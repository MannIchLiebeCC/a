@model a.Models.GP_Categories
@{
	ViewData["Title"] = "Create";
}
<div class="container p-3">
	<div class="row pt-4">
		<div class="col-6">
			<h2 class="text-primary">Create Area Reservation</h2>
		</div>
	</div>
	<br />
	<hr />
	<div class="row">
		<div class="col-md-4">
			<form method="post" asp-action="Create">
				<div class="form-group">
					<label class="control-label">Name of the Organization</label>
					<input asp-for="GP_Name" class="form-control" />
					<span asp-validation-for="GP_Name" class="text-danger"></span>
				</div>
				


				<br><br>
				<div class="form-group">
					<input type="submit" value="Create" class="btn btn-primary" />
				</div>
			</form>
		</div>
	</div>
	<div>
		<a asp-action="Index">Back to List</a>
	</div>
	</<div>
	</div>
</div>
@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}